<?xml version='1.0' encoding='utf-8'?>
<widget id="com.seniindonesia.kemendikbud" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <icon src="res/icon/seni-indonesia-apk.png" />
    <name>Seni Indonesia</name>
    <description>
        Seni Indonesia adalah aplikasi kesenian dan kebudayaan Indonesia yang berisikan konten macam-macam seni yang berada di Indonesia, terdapat kuis untuk menambah minat pengguna aplikasi.
    </description>
    <author email="romadhanedy@gmail.com" href="http://romadhanedy.ga">
        Romadhan Edy Prasetyo
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="cordova-sqlite-storage" spec="^3.2.1" />
    <plugin name="cordova-plugin-statusbar" spec="^2.4.3" />
    <engine name="android" spec="^7.1.4" />
</widget>
